pipeline{
	agent any
		stages{
			stage('One') {
				step {
					echo 'Hi, this is siva'
				}
			}
			satge('Two'){
				steps {
					inout('Do you want proceed?')
				}
			}
			satage('Three') {
				when {
					not {
						branch "master"
					}
				}
				steps {
					echo "Hello"
				}
			}
			stage('Four') {
					parallel {
						stage('Unit Test') {
								steps{
									echo"Running the unit test.."
								}
						}
						stage('Integration test') {
								agent {
									docker {
										reusenode false
										image 'ubuntu'
									}
								}
								steps {
									echo 'Running the integration test..'
								}
						}
					}
			}
			}
			}
									
